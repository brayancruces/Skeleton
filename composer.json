{
    "name": "draperstudio/:package_name",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "draperstudio",
        ":package_name"
    ],
    "homepage": "https://github.com/draperstudio/:package_name",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.5.9 || ^7.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DraperStudio\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\Tests\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
